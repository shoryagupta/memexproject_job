        $(document).ready(function() {                        
            function checkOffset() {
                if($('#footer-button').offset().top + $('#footer-button').outerHeight() >= $('#footer').offset().top){
                    $('#footer-button').css('position', 'absolute');
                }
                if($(window).scrollTop() + $(window).innerHeight() < $('#footer').offset().top){
                    $('#footer-button').css('position', 'fixed'); // restore when you scroll up
                }
            }
            
            //initialize in case footer is partially showing before the user scrolls.
            checkOffset();
            
            $(window).scroll(function() {
                checkOffset();
            });
            
            
            $('#footer-button').toggle(
                function() {
                    $('html, body').animate({
                        scrollTop: $("#footer").offset().top
                     }, 200);
                    $('span.footer-go-down').addClass('footer-go-up').removeClass('footer-go-down');
                },
                function() { 
                    $('html, body').animate({
                        scrollTop: $("#bodyTop").offset().top
                     }, 200);
                    $('span.footer-go-up').addClass('footer-go-down').removeClass('footer-go-up');

    //Validate Form Data
    function validateCommentForm() {
        resetCommentFormValidation();
        //Validate Form Using Validity Plugin
        $.validity.setup({ outputMode: "label" });
        $.validity.start();
        $("#commentForm .required").require();
        var result = $.validity.end();
        //Additional Validation required for Disclosure Input and Checkbox
        if (!$('#noConflict').is(':checked') && $('#txtDisclosure').val() == '') {
            $('.disclosure.error').show();
            return false;
        }
        else if (result.valid) return true;
        else return false;
    }
    //Capture Form Data and Send back to Server
    function processCommentForm() {
        $("#commentForm .pleaseWait").show();
        var oJSON = "{";
        $('#commentForm input[type=checkbox]').each(function () {
            var value =  $(this).is(':checked');           
            oJSON += '"' + $(this).attr('name') + '" :' + '"' + value + '"' + ", ";            
        });
        $('#commentForm input[type=text], #commentForm textarea').each(function () {
           var value = $(this).val();            
            var replacevalue=/"/g;
            var replacevaluewith='\\\"';
            if (typeof(value)== "string") {value=value.replace(replacevalue,replacevaluewith); }
             //Replace Double Quotes with &quot;
				//In the Callback event on the server, replace &quot; with "" after the JS Deserialization is complete.
				oJSON += '"' + $(this).attr('name') + '" :' + '"' + value.replace(/"/g, "&quot;") + '"' + ", ";
        });
        oJSON = $.trim(oJSON);
        oJSON = oJSON.substring(0, oJSON.length - 1);      
        oJSON += "}";
        CallBack(oJSON);
    }
